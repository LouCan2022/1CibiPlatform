{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "LoginEntryPoint": {
        "ClusterId": "onePlatform",
        "Match": {
          "Path": "/token/generatetoken",
          "Methods": [ "POST" ]
        },
        "Transforms": [
          {
            "PathSet": "/login"
          }
        ]
      },
      "LoginWebEntryPoint": {
        "ClusterId": "onePlatform",
        "Match": {
          "Path": "/token/web/generatetoken",
          "Methods": [ "POST" ]
        },
        "Transforms": [
          {
            "PathSet": "/loginweb"
          }
        ]
      },
      "LoginWebRefreshTokenEntryPoint": {
        "ClusterId": "onePlatform",
        "Match": {
          "Path": "/token/web/getnewaccesstoken",
          "Methods": [ "POST" ]
        },
        "Transforms": [
          {
            "PathSet": "/getnewaccesstoken"
          }
        ]
      },
      "IsAuthenticatedEntryPoint": {
        "ClusterId": "onePlatform",
        "Match": {
          "Path": "/auth/isAuthenticated",
          "Methods": [ "GET" ]
        },
        "Transforms": [
          {
            "PathSet": "/isauthenticated"
          }
        ]
      },
      "RegisterEntryPoint": {
        "ClusterId": "onePlatform",
        "Match": {
          "Path": "/auth/register",
          "Methods": [ "POST" ]
        },
        "Transforms": [
          {
            "PathSet": "/register"
          }
        ]
      },
      "VerifyOTPEntryPoint": {
        "ClusterId": "onePlatform",
        "Match": {
          "Path": "/auth/verify/otp",
          "Methods": [ "POST" ]
        },
        "Transforms": [
          {
            "PathSet": "/verify/otp"
          }
        ]
      },
      "IsVerifiedOTPEntryPoint": {
        "ClusterId": "onePlatform",
        "Match": {
          "Path": "/auth/validate/otp",
          "Methods": [ "POST" ]
        },
        "Transforms": [
          {
            "PathSet": "verify/validate/otp"
          }
        ]
      },
      "LogoutWebEntryPoint": {
        "ClusterId": "onePlatform",
        "Match": {
          "Path": "/auth/logout",
          "Methods": [ "POST" ]
        },
        "Transforms": [
          {
            "PathSet": "/logout"
          }
        ]
      },
      "SearchCandidateEntryPoint": {
        "ClusterId": "onePlatform",
        "Match": {
          "Path": "cnx/gettalkpushcandidate",
          "Methods": [ "GET" ]
        },
        "Transforms": [
          {
            "PathSet": "cnx/gettalkpushcandidate"
          }
        ]
      },
      "PartnerSystemQueryEntryPoint": {
        "ClusterId": "onePlatform",
        "Match": {
          "Path": "philsys/idv",
          "Methods": [ "POST" ]
        },
        "Transforms": [
          {
            "PathSet": "partnersystemquery"
          }
        ]
      },
      "UpdateFaceLivenessSessionEntryPoint": {
        "ClusterId": "onePlatform",
        "Match": {
          "Path": "philsys/idv/updatefacelivenesssession",
          "Methods": [ "POST" ]
        },
        "Transforms": [
          {
            "PathSet": "updatefacelivenesssession"
          }
        ]
      }
    },
    "Clusters": {
      "onePlatform": {
        "Destinations": {
          "backend1": {
            "Address": "http://apis:8080"
          }
        }
      }
    }
  }
}
       
      