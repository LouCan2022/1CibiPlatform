{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "LoginEntryPoint": {
        "ClusterId": "onePlatform",
        "Match": {
          "Path": "/token/generatetoken",
          "Methods": [ "POST" ]
        },
        "Transforms": [
          {
            "PathSet": "/login"
          }
        ]
      },
      "SearchCandidateEntryPoint": {
        "ClusterId": "onePlatform",
        "Match": {
          "Path": "/GetTalkpushCandidate",
          "Methods": [ "GET" ]
        },
        "Transforms": [
          {
            "PathSet": "/GetTalkpushCandidate"
          }
        ]
      },
      "FrontEndEntryPoint": {
        "ClusterId": "frontEnd",
        "Match": {
          "Path": "/{**catchall}"
        }
      }
    },
    "Clusters": {
      "onePlatform": {
        "HttpRequest": { "Version": "1.1" },
        "Destinations": {
          "backend1": {
            "Address": "http://apis:8080" // /token/generatetoken
          }
        }
      },
      "frontEnd": {
        "Destinations": {
          "frontend1": {
            "Address": "http://frontendUI:8080"
          }
        }
      }
    }
  }
}